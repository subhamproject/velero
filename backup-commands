
velero backup create sample-namespace-backup --include-namespaces sample --wait

 velero backup create subham --wait

velero backup logs sample-namespace-backup

velero get backups

velero restore create sample-namespace-backup --from-backup sample-namespace-backup --wait

velero backup logs sample-namespace-backup

velero get backup


velero backup create first --include-namespaces argocd --wait

